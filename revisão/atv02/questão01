#include <stdio.h>

int main(void) {
  float xa, xb, ya, yb, pa, pb, d;

  printf("informe xA: ");
  scanf("%f", &xa);
  printf("informe xb: ");
  scanf("%f", &xb);
  printf("informe ya: ");
  scanf("%f", &ya);
  printf("informe yb: ");
  scanf("%f", &yb);

  
  pa = (xb - xa) * (xb - xa);
  pb = (yb - ya) * (yb - xa);

  d = pa - pb;

  printf("a distancia entre o ponto A e o ponto B Ã© %.2f", d);
  
  return 0;
}
